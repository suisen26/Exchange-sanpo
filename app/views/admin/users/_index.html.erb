<table class="table">
  <% users.each do |user| %>
    <tr>
      <td>
        <%= link_to admin_user_path(user) do %>
          <%= image_tag user.get_profile_image(100, 100) %></br>
          <%= user.name %>
        <% end %>
      </td>
      <td>
        <%= link_to admin_user_path(user) do %>
          <%= user.introduction %>
        <% end %>
      </td>
      <td>
        <% if user.name != "ゲストユーザー" %>
          フォロー数: <%= link_to user.followings.count, admin_user_followings_path(user) %>
          フォロワー数: <%= link_to user.followers.count, admin_user_followers_path(user) %>
        <% end %>
      </td>
    </tr>
    
  <% end %>
</table>
      